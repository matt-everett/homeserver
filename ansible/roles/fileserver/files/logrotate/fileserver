/var/log/duplicity.log
/var/log/snapshot.log
{
    su root root
    rotate 10
    yearly
    missingok
    notifempty
    nocompress
    extension log
    dateext
}

/var/log/health-check.log
{
    su root root
    rotate 12
    monthly
    missingok
    notifempty
    nocompress
    extension log
    dateext
}
